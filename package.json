{
  "name": "eslint-config-uphold",
  "version": "6.7.1",
  "description": "Uphold-flavored ESLint config",
  "keywords": [
    "config",
    "eslint",
    "lint",
    "shared"
  ],
  "homepage": "https://github.com/uphold/eslint-config-uphold#readme",
  "bugs": {
    "url": "https://github.com/uphold/eslint-config-uphold/issues"
  },
  "license": "MIT",
  "author": "Uphold",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uphold/eslint-config-uphold.git"
  },
  "exports": {
    ".": "./src/index.js",
    "./configs" : {
      "import": "./src/configs/index.mjs",
      "require": "./src/configs/index.cjs"
    }
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint",
    "release": "release-it",
    "test": "node --test --experimental-test-coverage 'test/**/*.test.js'",
    "test:snapshot": "node --test --test-update-snapshots 'test/**/*.test.js'",
    "test:watch": "node --test --watch 'test/**/*.test.js'"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^52.0.1",
    "eslint-plugin-mocha": "^11.1.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-imports-requires": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sql-template": "^3.1.0",
    "globals": "^16.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@fastify/pre-commit": "^2.2.0",
    "@types/node": "^22.17.0",
    "@uphold/github-changelog-generator": "^4.0.2",
    "eslint": "~9.32.0",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "prettier": "^3.6.2",
    "react": "^19.0.0",
    "release-it": "^19.0.4"
  },
  "peerDependencies": {
    "eslint": "~9.32.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "prettier": "^3.0.0",
    "react": "^19.0.0",
    "typescript": ">4.8.4 <5.9.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import-x": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-sort-class-members": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || >=23"
  },
  "type": "module"
}
