{
  "name": "eslint-config-uphold",
  "version": "6.0.0",
  "description": "Uphold-flavored ESLint config",
  "keywords": [
    "config",
    "eslint",
    "lint",
    "shared"
  ],
  "homepage": "https://github.com/uphold/eslint-config-uphold#readme",
  "bugs": {
    "url": "https://github.com/uphold/eslint-config-uphold/issues"
  },
  "license": "MIT",
  "author": "Uphold",
  "main": "src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uphold/eslint-config-uphold.git"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint src test/index.js",
    "release": "release-it",
    "test": "mocha $npm_package_options_mocha"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.26.10",
    "@eslint/js": "^9.22.0",
    "@stylistic/eslint-plugin-js": "2.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "50.6.8",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-imports-requires": "2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sql-template": "^3.1.0",
    "globals": "^15.15.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@uphold/github-changelog-generator": "^3.4.0",
    "eslint": "9.22.0",
    "mocha": "^10.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "3.5.3",
    "release-it": "^17.11.0",
    "should": "^13.2.3"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "prettier": ">=3.0.0"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": ">=20"
  },
  "options": {
    "mocha": "-t 10000 --require should test"
  }
}
